{
  "name": "git-diff-details",
  "main": "./lib/main",
  "version": "1.4.0",
  "description": "View git diffs directly in atom.",
  "keywords": [
    "git",
    "diff",
    "details",
    "inline",
    "undo"
  ],
  "repository": "https://github.com/samu/git-diff-details",
  "license": "MIT",
  "engines": {
    "atom": ">=1.7.0"
  },
  "activationCommands": {
    "atom-text-editor": ["git-diff-details:toggle-git-diff-details"]
  },
  "configSchema": {
    "closeAfterCopy": {
      "type": "boolean",
      "default": false,
      "title": "Close diff view after copy"
    },
    "keepViewToggled": {
      "type": "boolean",
      "default": true,
      "title": "Keep view toggled when leaving a diff"
    },
    "enableSyntaxHighlighting": {
      "type": "boolean",
      "default": false,
      "title": "Enable syntax highlighting in diff view"
    },
    "showWordDiffs": {
      "type": "boolean",
      "default": true,
      "title": "Show word diffs"
    }
  },
  "dependencies": {
    "atom-space-pen-views": "^2.0.3",
    "diff": "^2.2.2"
  },
  "devDependencies": {
    "prettier": "^1.9.2"
  }
}
